    "#Clean the data to only include necessary columns and drop certain row values\n",
    "df = df[['NAICS2017_LABEL','RACE_GROUP_LABEL','SEX_LABEL','PAYANN','EMP']]\n",
    "df = df[df.RACE_GROUP_LABEL.isin([\"Nonminority\",\"Minority\",\"Equally minority/nonminority\"])]\n",
    "df = df[~df.SEX_LABEL.isin([\"Classifiable\",\"Total\",\"Unclassifiable\",])]\n",
    "\n",
    "#Creates a bar chart showing the race breakdown of businesses compared to annual payroll\n",
    "race_and_employees = df[['RACE_GROUP_LABEL','PAYANN']]\n",
    "\n",
    "reclean_vc = race_and_employees.RACE_GROUP_LABEL.value_counts()\n",
    "\n",
    "fig1 = reclean_vc.plot.bar(title=\"Annual Payroll by Race of Employees\",ylabel='Annual Payroll ($1000s)',xlabel='Majority Race of Business')\n"
